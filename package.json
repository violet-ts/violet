{
  "name": "violet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:client": "next dev -p 3000",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "build": "run-p build:client build:server",
    "build:client": "aspida && prisma generate --schema server/prisma/schema.prisma && next build && next export",
    "build:server": "npm run build --prefix server",
    "build:types": "aspida && npm run build:frourio --prefix server",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "test": "npm run build:types && jest",
    "typecheck": "npm run build:types && tsc --noEmit && tsc --noEmit -p server"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "dependencies": {
    "@aspida/axios": "^1.2.1",
    "@aspida/swr": "^1.2.1",
    "axios": "^0.21.1",
    "class-validator": "^0.12.2",
    "firebase": "^8.0.1",
    "next": "^10.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.14.0",
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "fastify": "^3.9.2",
    "fastify-cors": "^5.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "tailwindcss": "1.9.6",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
