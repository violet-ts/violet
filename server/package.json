{
  "name": "violet-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && npm run prisma:seed && frourio && run-p dev:*",
    "dev:server": "webpack --watch --mode=development",
    "dev:frourio": "frourio --watch",
    "dev:prisma": "prisma generate --watch",
    "build": "npm run build:frourio && webpack --mode=production",
    "build:frourio": "npm run migrate:dev && npm run prisma:seed && prisma generate && frourio",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "start": "cross-env NODE_ENV=production node index.js",
    "prisma:seed": "prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.29.0",
    "@aws-sdk/credential-providers": "^3.38.0",
    "@aws-sdk/s3-request-presigner": "^3.30.0",
    "@prisma/client": "3.3.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "fastify": "^3.20.2",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^5.3.2",
    "fastify-jwt": "^3.0.1",
    "fastify-multipart": "^4.0.7",
    "fastify-static": "^4.2.3",
    "uuid": "^8.3.2",
    "velona": "^0.7.0"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.4",
    "@types/jest": "^27.0.1",
    "@types/uuid": "^8.3.1",
    "cross-env": "^7.0.3",
    "frourio": "^0.25.1",
    "nodemon-webpack-plugin": "^4.5.2",
    "npm-run-all": "^4.1.5",
    "prisma": "3.3.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
